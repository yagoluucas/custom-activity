{
  "workflowApiVersion": "1.0",
  "metaData": {
    "icon": "images/icon.png",
    "category": "customer"
  },
  "type": "REST",
  "lang": {
    "pt-BR": {
      "name": "Inserir Contatos",
      "description": "activity que insere contatos em uma data extension"
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "email": "{{Contact.Default.Email}}"
        },
        {
          "contactKey": "{{Contact.Key}}"
        }
      ],
      "outArguments": [
        {
          "dafault": "ok"
        }
      ],
      "timeout": 10000,
      "retryCount": 3,
      "retryDelay": 2000,
      "url": "https://custom-activity-zeta.vercel.app/api/execute"
    }
  },
  "configurationArguments": {
    "applicationExtensionKey": "bbcfc53d-ef0e-4e80-8297-748608582855"
  },
  "userInterfaces": {
    "configModal": {
      "height": 300,
      "width": 300
    }
  }
}