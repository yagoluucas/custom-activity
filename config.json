{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "images/icon.png",
    "category": "customer"
  },
  "type": "REST",
  "lang": {
    "pt-BR": {
      "name": "Insert Contact",
      "description": "activity que insere contatos em uma data extension"
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "email": "{{Contact.Default.Email}}"
        },
        {
          "contactKey": "{{Contact.Key}}"
        }
      ],
      "outArguments": [
                {
          "returnValue": "default"
        }
      ],
      "timeout": 10000,
      "retryCount": 3,
      "retryDelay": 2000,
      "url": "https://custom-activity-zeta.vercel.app/api/execute"
    }
  },
  "configurationArguments": {
    "applicationExtensionKey": "bc95763f-cfa3-4d2a-a8ce-05e184c87cd8",
    "save": {
      "url": "https://custom-activity-zeta.vercel.app/api/save"
    },
    "publish": {
      "url": "https://custom-activity-zeta.vercel.app/api/publish"
    },
    "validate": {
      "url": "https://custom-activity-zeta.vercel.app/api/validate"
    },
    "stop": {
      "url": "https://custom-activity-zeta.vercel.app/api/stop"
    }
  },
  "userInterfaces": {
    "configModal": {
      "height": 500,
      "width": 500,
      "fullscreen": true
    }
  }
}