{
  "workflowApiVersion": "1.1",
  "metaData": {
    "icon": "https://custom-activity-zeta.vercel.app/images/icon.svg",
    "category": "customer",
    "expressionBuilderPrefix": "InsertContact",
    "configOnDrop": true
  },
  "type": "REST",
  "lang": {
    "pt-BR": {
      "name": "Insert Contact",
      "description": "Teste de Custom Activity"
    }
  },
  "userInterfaces": {
    "configModal": {
      "height": 400,
      "width": 600
    }
  },
  "arguments": {
    "execute": {
      "inArguments": [
        {
          "contactKey": "{{Context.ContactKey}}"
        }
      ],
      "outArguments": [
        {
          "resultado": "success"
        }
      ],
      "url": "https://custom-activity-zeta.vercel.app/api/execute.js"
    }
  }
}
