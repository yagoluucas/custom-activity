{
  "version": 2,
  "public": true,
"headers": [
    {
      "source": "/config.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/customActivity.js", "destination": "/customActivity.js" },
    { "source": "/style.css", "destination": "/style.css" },
    { "source": "/images/icon.svg", "destination": "/images/icon.svg" },
    { "source": "/config.json", "destination": "/config.json" }
  ]
}