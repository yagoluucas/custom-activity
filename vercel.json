{
  "version": 2,
  "public": true,
"headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites":[
    { "source": "/", "destination": "/index.html" }
  ]
}